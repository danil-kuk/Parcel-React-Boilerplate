{
  "name": "parcel-react-boilerplate",
  "version": "1.0.0",
  "description": "Parcel React Boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html",
    "deploy-build": "env PUBLIC_URL=/<RepoName>/ parcel build ./src/index.html --public-url /<RepoName>/",
    "deploy": "./deploy.sh",
    "lint": "eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "parcel-bundler": "^1.12.4"
  }
}
